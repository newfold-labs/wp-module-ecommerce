{
  "name": "@newfold/wp-module-ecommerce",
  "description": "Brand Agnostic eCommerce Experience",
  "license": "GPL-2.0-or-later",
  "version": "1.7.2",
  "main": "build/index.js",
  "files": [
    "build/",
    "*.d.ts",
    "*.css"
  ],
  "types": "index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "set-version-bump": "node ./set-version-bump.js && npm i && rm -rf ./build && npm run build && composer run i18n",
    "set-version-minor": "node ./set-version-bump.js minor && npm i && rm -rf ./build && npm run build && composer run i18n",
    "build": "wp-scripts build ./src/index.js --config ./scripts/webpack.config.js",
    "lint": "wp-scripts lint-js ./src/index.js",
    "start": "wp-scripts start ./src/index.js --config ./scripts/webpack.config.js",
    "test:unit": "echo \"No tests implemented\"",
    "test:e2e": "echo \"No tests implemented\""
  },
  "dependencies": {
    "@faizaanceg/pandora": "^1.1.1",
    "@heroicons/react": "2.2.0",
    "@newfold/js-utility-ui-analytics": "^1.3.1",
    "@newfold/ui-component-library": "^1.3.0",
    "@newfold/wp-module-runtime": "^1.1.3",
    "@reduxjs/toolkit": "2.6.1",
    "@wordpress/api-fetch": "^7.21.0",
    "@wordpress/components": "^29.6.0",
    "@wordpress/data": "^10.20.0",
    "@wordpress/date": "^5.22.0",
    "@wordpress/element": "^6.22.0",
    "@wordpress/i18n": "^5.11.0",
    "classnames": "^2.5.1",
    "moment": "^2.30.1",
    "swr": "2.3.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.8",
    "@testing-library/cypress": "^10.0.3",
    "@wordpress/dom-ready": "^4.22.0",
    "@wordpress/env": "^10.22.0",
    "@wordpress/scripts": "^27",
    "cypress": "^14.3.0",
    "cypress-axe": "^1.6.0",
    "cypress-iframe": "^1.0.1",
    "tailwindcss": "^3.4.4"
  }
}